{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "installCommand": "echo 'No dependencies to install'",
  "buildCommand": "echo 'No build needed'",
  "outputDirectory": "."
}